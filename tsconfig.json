{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@context/*": ["context/*"],
      "@atoms/*": ["atoms/*"],
      "@molecules/*": ["molecules/*"],
      "@images/*": ["images/*"],
      "@documents/*": ["documents/*"],
      "@styles/*": ["styles/*"],
      "@svg/*": ["svg/*"],
      "@utils/*": ["utils/*"],
      "@redux/*": ["redux/*"],
      "@src/*": ["*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
